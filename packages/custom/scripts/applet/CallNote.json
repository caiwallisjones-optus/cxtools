    "7": {
     "name": "ActionValue",
     "value": "\r\n<style>\r\n.form-container {CHAR(123)}\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n  margin: 20px;\r\n{CHAR(125)}\r\n\r\nform {CHAR(123)}\r\n  padding: 20px;\r\n{CHAR(125)}\r\n</style>\r\n\r\n<div class = \"form-container\">\r\n  <div class=\"row\" class=\"form-container\">\r\n    <div class=\"col-md-6\">\r\n      <label for=\"IDofInput\">Name of caller:</label> \r\n      <input type=\"text\" id=\"P1\" />\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n     <label for=\"IDofInput\">If not client, is caller authorised:</label> \r\n     <input type=\"radio\" id=\"Yes\" name=\"P2\" value=\"Yes\">\r\n     <label for=\"yes\">Yes</label><br>\r\n     <input type=\"radio\" id=\"No\" name=\"P2\" value=\"No\">\r\n     <label for=\"no\">No</label><br>\r\n     <input type=\"radio\" id=\"NA\" name=\"P2\" value=\"NA\">\r\n  <label for=\"na\">N/A</label>\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <label for=\"IDofInput\">Account ID:</label> \r\n      <input type=\"text\" id=\"P3\" />\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <label for=\"IDofInput\">Verified:</label> \r\n      <input type=\"text\" id=\"P4\" />\r\n    </div>\r\n    <div class=\"col-md-6\">\r\n      <label for=\"IDofInput\">Trigger Number:</label> \r\n      <input type=\"text\" id=\"P5\" />\r\n    </div>\r\n    \r\n  </div>\r\n</div>\r\n\r\n<input type=\"submit\" value=\"Tag Notes\"  class=\"form-container\"/>\r\n"
    }
