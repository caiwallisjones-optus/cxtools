n      <Validators />\r\n      <IsPasswordActual>false</IsPasswordActual>\r\n      <MaxLengthActual>0</MaxLengthActual>\r\n      <SizeActual>0</SizeActual>\r\n    </ItemParent>\r\n    <ItemParent xsi:type=\"CheckBox\" Class=\"\" ID=\"Verified\" Name=\"Verified\">\r\n      <Label>\r\n        <Expression>Verified</Expression>\r\n      </Label>\r\n      <Checked>\r\n        <Expression>False</Expression>\r\n      </Checked>\r\n      <Value>\r\n        <Expression>Yes</Expression>\r\n      </Value>\r\n    </ItemParent>\r\n    <ItemParent xsi:type=\"InputField\" Class=\"\" ID=\"\" Name=\"TriggerNo\">\r\n      <Label>\r\n        <Expression>Trigger Number: &lt;br&gt;</Expression>\r\n      </Label>\r\n      <DefaultValue>\r\n        <Expression />\r\n      </DefaultValue>\r\n      <IsPassword>\r\n        <Expression>False</Expression>\r\n      </IsPassword>\r\n      <MaxLength>\r\n        <Expression>-1</Expression>\r\n      </MaxLength>\r\n      <Size>\r\n        <Expression>-1</Expression>\r\n      </Size>\r\n      <Validators />\r\n      <IsPasswordActual>false</IsPasswordActual>\r\n      <MaxLengthActual>0</MaxLengthActual>\r\n      <SizeActual>0</SizeActual>\r\n    </ItemParent>\r\n    <ItemParent xsi:type=\"Paragraph\" Class=\"\" ID=\"\">\r\n      <Text>\r\n        <Expression>&lt;b style=\"color: red\"&gt; ** Click on NEXT before transferring call **</Expression>\r\n      </Text>\r\n    </ItemParent>\r\n  </Components>\r\n  <ShowBack>\r\n    <Expression>False</Expression>\r\n  </ShowBack>\r\n  <ShowCancel>\r\n    <Expression>False</Expression>\r\n  </ShowCancel>\r\n  <ShowHold>\r\n    <Expression>False</Expression>\r\n  </ShowHold>\r\n  <ShowNext>\r\n    <Expression>True</Expression>\r\n  </ShowNext>\r\n  <ShowTransfer>\r\n    <Expression>False</Expression>\r\n  </ShowTransfer>\r\n  <Title>\r\n    <Expression>Call Notes</Expression>\r\n  </Title>\r\n  <ShowBackActual>false</ShowBackActual>\r\n  <ShowCancelActual>false</ShowCancelActual>\r\n  <ShowHoldActual>false</S